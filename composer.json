{
  "name": "rcm/i18n",
  "description": "Stores translations in DB for ZF2 i18n",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "reliv",
    "plugins",
    "module",
    "zf2"
  ],
  "homepage": "http://www.reliv.com/",
  "authors": [
    {
      "name": "Rod McNew",
      "email": "rmcnew@relivinc.com",
      "homepage": "http://www.reliv.com/"
    },
    {
      "name": "James Jervis",
      "email": "jjervis@relivinc.com",
      "homepage": "https://github.com/reliv"
    }
  ],
  "suggest": {
    "bower-asset/angular-translate": ">=2.7.2",
  },
  "require": {
    "fxp/composer-asset-plugin": "^1",
    "php": ">=5.4",
    "psr/container": "^1",
    "psr/http-message": "^1",
    "rcm/core": "^1.20.0",
    "rcm/core-js": "^1.0.0",
    "rcm/html-purifier": "^1.2",
    "rcm/user": "^1",
    "zendframework/zend-diactoros": "^1",
    "zendframework/zend-eventmanager": "^2",
    "zendframework/zend-filter": "^2",
    "zendframework/zend-form": "^2",
    "zendframework/zend-http": "^2",
    "zendframework/zend-i18n": "^2",
    "zendframework/zend-inputfilter": "^2",
    "zendframework/zend-mvc": "^2",
    "zendframework/zend-servicemanager": "2.* || 3.*",
    "zendframework/zend-stdlib": "^2",
    "zendframework/zend-validator": "^2",
    "zendframework/zend-view": "^2"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "RcmI18n\\": "src/"
    }
  },
  "extra": {
    "asset-pattern-skip-version": "(-patch*)",
    "asset-vcs-driver-options": {
      "github-no-api": true
    }
  },
  "config": {
    "asset-pattern-skip-version": "(-patch*)",
    "asset-installer-paths": {
      "bower-asset-library": "public/bower_components"
    }
  }
}
